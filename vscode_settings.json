        {
            "before": ["e"],
            "after": ["<up>"]
        },
        {
            "before": ["i"],
            "after": ["<right>"]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["k"],
            "after": ["n"]
        },
        {
            "before": ["K"],
            "after": ["N"]
