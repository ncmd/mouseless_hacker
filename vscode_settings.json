        {
            "before": ["I"],
            "after": ["L"]
        },
        {
            "before": ["j"],
            "after": ["e"]
        },
        {
            "before": ["J"],
            "after": ["E"]
        }
    ],
    "vim.handleKeys": {
        "<D-d>": false,
        "<C-x>": false,
        "<C-a>": false,
        "<C-f>": false,
        "<C-c>": false,
        "<C-v>": false
    },
    "vim.cursorStylePerMode.insert": "block",
    "vim.cursorStylePerMode.normal": "block-outline",
    "vim.cursorStylePerMode.visual": "line",
    "workbench.colorCustomizations": {
        "statusBar.background": "#272822",
        "statusBar.noFolderBackground": "#272822",
        "statusBar.debuggingBackground": "#272822",
        "tab.activeBorder": "#F92672",
        "tab.unfocusedActiveBorder": "#000000"
    },
    "editor.multiCursorModifier": "alt",
    "vim.camelCaseMotion.enable": true,
    "vim.searchHighlightColor": "#66D9EF",
    "vim.searchHighlightTextColor": "#272822",
    "vim.sneak": true,
    "vim.easymotionMarkerForegroundColorOneChar": "#F92672",
    "vim.easymotionMarkerForegroundColorTwoChar": "#F92672",
    "terminal.external.osxExec": "Alacritty.app",
    "editor.minimap.enabled": false,
    "bracketPairColorizer.timeOut": 0,
    "bracketPairColorizer.showBracketsInGutter": true,
    "bracketPairColorizer.showBracketsInRuler": true,
    "editor.suggestSelection": "first",
    "editor.autoClosingQuotes": "never",
    "editor.autoClosingBrackets": "never",
    "python.jediEnabled": false,
    "vim.scroll": 10,
    "vim.useSystemClipboard": true,
    "vim.debug.silent": true,
    "typescript.check.npmIsInstalled": false,
    "css.validate": false,
    "typescript.validate.enable": false,
    "javascript.validate.enable": false,
    "php.validate.enable": false,
    "html.validate.scripts": false,
    "html.validate.styles": false,
    "less.validate": false,
    "scss.validate": false,
    "vim.enableNeovim": true,
    "vim.neovimPath": "/usr/local/bin/nvim",
    "editor.maxTokenizationLineLength": 100,
    "json.maxItemsComputed": 100,
    "vim.experimentalOptimizations": true,
    "editor.accessibilityPageSize": 1,
    "css.lint.emptyRules": "ignore",
    "css.lint.argumentsInColorFunction": "ignore",
    "css.lint.fontFaceProperties": "ignore",
    "css.lint.hexColorLength": "ignore",
    "css.lint.propertyIgnoredDueToDisplay": "ignore",
    "css.lint.unknownAtRules": "ignore",
    "css.lint.unknownProperties": "ignore",
    "css.lint.vendorPrefix": "ignore",
    "less.lint.argumentsInColorFunction": "ignore",
    "less.lint.emptyRules": "ignore",
    "less.lint.hexColorLength": "ignore",
    "less.lint.unknownAtRules": "ignore",
    "less.lint.unknownProperties": "ignore",
    "less.lint.vendorPrefix": "ignore",
    "scss.lint.argumentsInColorFunction": "ignore",
    "scss.lint.emptyRules": "ignore",
    "scss.lint.fontFaceProperties": "ignore",
    "scss.lint.hexColorLength": "ignore",
    "scss.lint.propertyIgnoredDueToDisplay": "ignore",
    "scss.lint.unknownAtRules": "ignore",
    "scss.lint.unknownProperties": "ignore",
    "scss.lint.vendorPrefix": "ignore",
    "git.autorefresh": false,
    "git.autoRepositoryDetection": false,
    "git.confirmEmptyCommits": false,
    "git.enabled": false,
    "git.decorations.enabled": false,
    "git.detectSubmodules": false,
    "git.enableStatusBarSync": false,
    "debug.console.historySuggestions": false,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "keyboard.touchbar.enabled": false,
    "problems.autoReveal": false,
