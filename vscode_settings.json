        {
            "before": ["j"],
            "after": ["e"]
        },
        {
            "before": ["J"],
            "after": ["E"]
        }
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        {
            "before": ["k"],
            "after": ["n"]
        },
        {
            "before": ["K"],
            "after": ["N"]
        },
        {
            "before": ["u"],
            "after": ["i"]
        },
        {
            "before": ["U"],
            "after": ["I"]
        },
        {
            "before": ["l"],
            "after": ["u"]
        },
        {
            "before": ["L"],
            "after": ["U"]
        },
        {
            "before": ["N"],
            "after": ["J"]
        },
        {
            "before": ["E"],
            "after": ["K"]
