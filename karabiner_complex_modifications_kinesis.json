    {
      "description": "Quit App by pressing command-q twice then hold",
      "manipulators": [
        {
          "type": "basic",
          "conditions": [
            {
              "type": "variable_if",
              "name": "command-q",
              "value": 1
            },
            {
              "type": "frontmost_application_if",
              "bundle_identifiers": [
                "^io\\.alacritty$"
              ]
            }
          ],
          "from": {
            "key_code": "q",
            "modifiers": {
              "mandatory": [
                "command"
