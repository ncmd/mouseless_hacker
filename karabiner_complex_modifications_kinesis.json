    {
      "description": "backspace to left_command",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "delete_or_backspace",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_command",
              "repeat": true
            }
          ],
          "to_if_alone": [
            {
              "key_code": "delete_or_backspace",
              "repeat": false
            }
          ],
          "to_if_held_down": [
            {
              "key_code": "left_command",
              "repeat": true
            }
          ]
        }
      ]
